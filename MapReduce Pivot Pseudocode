map(row_index, values):
   
      for (column_index = 1 to length values):
        
           k = column_index
           v ={ row_index :values[column_index]}
      return{ k : v }
      
      
 Pivotvalues = []
 
 for ( a in v[]) :
 
      Pivotvalues[a.k] = a.value
      Pivot_index = k
      
      return {Pivot_index ,Pivotvalues }
      
           
